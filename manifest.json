{
    "manifest_version": 3,
    "name": "Privacy Inspector",
    "version": "1.0",
    "description": "Visualisez les données collectées par les sites web",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }]
  }